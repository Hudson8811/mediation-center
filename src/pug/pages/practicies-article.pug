extends ../layouts/default

block vars
  -
    const page = {
    title: 'МЕДИАЦИЯ В СФЕРЕ РИТЕЙЛА И FMCG',
    };

block main
  section.practicies.practicies-article
    .container
      .practicies-heading.practicies__heading.practicies__heading--article
        +title('Практики', 'h1', 'practicies-heading__title title--upper')
      
      .practicies-wrapper
        .practicies-wrapper__section.practicies-wrapper__section--left
          .practicies-article__heading
            +title('МЕДИАЦИЯ В&nbsp;СФЕРЕ&nbsp;РИТЕЙЛА И&nbsp;FMCG', 'h2', 'practicies__article-title practicies-article__heading-title title--upper')

          .person-card.practicies-article-author.practicies-article__author
            .practicies-article-author__info
              img(src="images/personal/1.jpg", srcset="images/personal/1@2x.jpg 2x", alt="Анна сорокина. Руководитель практики «Медиация в сфере ритейла и FMCG»").practicies-article-author__info-photo

              .practicies-article-author__info-description
                .person-card__name.practicies-article-author__info-name
                  | Анна сорокина
                
                .person-card__text.practicies-article-author__info-text.text-block
                  p <b>Руководитель практики «Медиация в сфере ритейла и FMCG»</b>
                  br
                  p Руководитель Центра медиации при РСПП, юрист, психолог, коуч, медиатор, тренер медиаторов
                  br
                  p Москва, Московская область

            .practicies-article-author__buttons
              a(href="#").button.practicies-article-author__buttons-item.practicies-article-author__buttons-item--primary
                | отраслевой
                br
                | экспертный совет
              a(href="#").button.practicies-article-author__buttons-item.practicies-article-author__buttons-item--secondary
                | медиаторы практики
              a(href="#").button.practicies-article-author__buttons-item.practicies-article-author__buttons-item--secondary
                | эксперты практики


